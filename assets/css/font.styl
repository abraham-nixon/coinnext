/* Icons */
@font-face
  font-family 'icons'
  src:url(CDN('/fonts/icons.eot'))
  src:url(CDN('/fonts/icons.eot?#iefix')) format('embedded-opentype'),
    url(CDN('/fonts/icons.ttf')) format('truetype'),
    url(CDN('/fonts/icons.woff')) format('woff'),
    url(CDN('/fonts/icons.svg#icons')) format('svg')
  font-weight normal
  font-style normal

[class^="icon-"], [class*=" icon-"]
  font-family 'icons'
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.icon-list-ul:before
  content: "\e600"
.icon-reorder:before
  content: "\e604"
.icon-hide:before
  content: "\e601"
.icon-lock:before
  content: "\e603"
.icon-bolt:before
  content: "\e605"
.icon-shield:before
  content: "\e606"
.icon-thumbs-up:before
  content: "\e607"
.icon-circle-arrow-up:before
  content: "\e60a"
.icon-circle-arrow-down:before
  content: "\e60b"
.icon-envelope:before
  content: "\e602"
.icon-support:before
  content: "\e608"
.icon-search:before
  content: "\e609"